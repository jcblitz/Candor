<h1>Dashboard</h1>

Hello <%= @employee.employee_name %>

<h2>Sessions</h2>

<% @employee.interview_sessions.each do |interview_session| %>
    <p><%= interview_session.start_time.strftime("%I:%M %p")%> - <%= interview_session.end_time.strftime("%I:%M %p")%></p>
    <p>Location: <%= interview_session.location %></p>

    <p><%= link_to(interview_session.candidate.candidate_name, interview_url(interview_session.candidate.id)) %></p>
<% end -%>